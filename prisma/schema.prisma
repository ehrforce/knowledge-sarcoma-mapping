generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Configuration {
  id          String   @id @default(cuid())
  mappingUrl  String   // URL to the mapping.xlsx in Vercel Blob
  mappingName String   // Original filename
  icd10Url    String   // URL to the icd10.xlsx in Vercel Blob
  icd10Name   String   // Original filename
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  isActive    Boolean  @default(true)
}
